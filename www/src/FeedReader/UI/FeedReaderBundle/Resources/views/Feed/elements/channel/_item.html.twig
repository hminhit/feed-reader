<div data-channel-item-container>
    <div data-coordinate-group="channel-item"
         data-prototype="{{ include('FeedReaderBundle:Feed:elements/channel/item/_item.html.twig', {feedChannelItemForm: feedChannelForm.channelItem.vars.prototype})|e('html_attr') }}">
        {% for feedChannelItemForm in feedChannelForm.channelItem %}
            {% include('FeedReaderBundle:Feed:elements/channel/item/_item.html.twig') %}
        {% endfor %}
    </div>
    <div class="form-group">
        <label class="col-md-1 control-label"></label>
        <div class="col-md-11">
            <button type="button" class="btn btn-primary" data-btn-add-channel-item>Add a new item <i
                        class="glyphicon glyphicon-plus"></i>
            </button>
        </div>
    </div>
</div>